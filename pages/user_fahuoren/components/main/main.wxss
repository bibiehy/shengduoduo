.pagemain-box { display:flex; width:100vw; height:100vh; overflow:hidden; }
.pagemain-box .scrollviewbox { flex:1; width:100%; min-height:100%; }

.main-swiper { z-index:30; position:relative; width:100%; --td-swiper-radius:0; } /* --td-swiper-nav-dot-size:0; f3f3f3 */
.main-swiper .radian01 { position:absolute; left:-20%; bottom:-170px; width:140%; height:200px; background-color:#f3f3f3; border-top-left-radius:45%; border-top-right-radius:45%; }

.main-outerbox { padding-bottom:150px; overflow:hidden; } /* 解决 margin-bottom 无效的问题 */
.main-process { z-index:50; position:relative; background-color:#fff; border-radius:8px; padding:12px; margin:10px 10px 0; }
.main-process .header { display:flex; align-items:center; justify-content:space-between; padding:0 5px; }
.main-process .header .title { color:rgba(0, 0, 0, 0.75); font-size:17px; font-weight:600; }
.main-process .header .title .number { color:rgba(0, 0, 0, 0.45); font-size:14px; font-weight:400; padding-left:5px; }
.main-process .header .refresh { display:flex; align-items:center; font-size:16px; color:#1677ff; }
.main-process .listitem { position:relative; border:solid 1px #ebebeb; background-color:#fff; border-radius:8px; padding:10px 12px; margin-top:8px; margin-bottom:10px; }
.main-process .listitem .status { position:absolute; right:12px; top:12px; }
.main-process .listitem .mainbox .psname { width:80%; font-size:16px; color:rgba(0, 0, 0, 0.85); white-space:nowrap; text-overflow:ellipsis; overflow:hidden; }
.main-process .listitem .mainbox .pstime { font-size:12px; color:rgba(0, 0, 0, 0.45); margin-top:4px; }
.main-process .listitem .mainbox .psinfo { font-size:14px; color:rgba(0, 0, 0, 0.55); margin-top:4px; }
.main-process .listitem .footbox { display:flex; align-items:center; justify-content:space-between; padding-top:8px; }
.main-process .listitem .footbox .psbtns { display:flex; }
.main-process .listitem .footbox .psbtns .listag { display:flex; justify-content:center; min-width:70px !important; margin-right:10px; }
.main-process .listitem .footbox .yunfei { font-size:14px; color:#ff3b30; }
.main-process .nodata { height:380px; }



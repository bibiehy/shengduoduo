.pagemain-box { display:flex; flex-direction:column; width:100vw; height:100vh; overflow:hidden; }
.pagemain-box .scrollviewbox { flex:1; width:100%; height:100%; padding-bottom:150px; }

.main-swiper { z-index:30; position:relative; width:100%; --td-swiper-radius:0; } /* --td-swiper-nav-dot-size:0; f3f3f3 */
.main-swiper .radian01 { position:absolute; left:-20%; bottom:-170px; width:140%; height:200px; background-color:#f3f3f3; border-top-left-radius:45%; border-top-right-radius:45%; }

.main-process { z-index:50; position:relative; background-color:#fff; border-radius:8px; padding:15px 20px; margin:10px 10px 0; }
.main-process .title { color:rgba(0, 0, 0, 0.75); font-size:17px; font-weight:600; }
.main-process .title .number { color:rgba(0, 0, 0, 0.45); font-size:15px; font-weight:400; padding-left:10px; }

.popup-printbox { display:flex; flex-direction:column; width:100%; background-color:#fff; border-top-left-radius:12px; border-top-right-radius:12px; }
.popup-printbox .printheader { display:flex; align-items:center; height:50px;  }
.popup-printbox .printheader .cancelbtn { font-size:16px; color:#1677ff; padding:10px 16px; }
.popup-printbox .printheader .surebtn { font-size:16px; color:#1677ff; padding:10px 16px; }
.popup-printbox .printheader .poptitle { flex:1; text-align:center; font-size:17px; color:rgba(43, 18, 18, 0.8); }
.popup-printbox .printsearch { z-index:10; position:relative; box-shadow:0 5px 5px #ececec; padding:5px 12px; }
.popup-printbox .print-scroll { flex:1; background-color: #f3f3f3; padding:10px; }
.print-scroll .collapseitem { background-color:#fff; border-bottom:solid 1px #f0f0f0; padding:2px 15px; }
.print-scroll .collapseitem .collheader { display:flex; align-items:center; justify-content:space-between; --td-checkbox-icon-size:23px; --td-checkbox-font-size:16px; --td-checkbox-title-color:#8a8989; }
.print-scroll .collapseitem .collheader .guigename { font-size:17px; color:rgba(0, 0, 0, 0.85); }
.print-scroll .collapseitem .collheader .actionbox { display:flex; align-items:center; }
.print-scroll .collapseitem .collheader .actionbox .line { width:1px; height:16px; background-color:rgb(204, 202, 202); margin-left:15px; }
.print-scroll .collapseitem .collbody { --td-checkbox-icon-size:23px; --td-checkbox-font-size:15px; --td-checkbox-title-color:#696969; }
.print-scroll .collapseitem .collbody .subcont { position:relative; display:inline-block; width:calc(20% - 10px); height:32px; line-height:32px; color:#333; font-size:15px; background-color:#f5f5f5; border-radius:4px; text-align:center; box-sizing:border-box; margin:5px 5px 0; }
.print-scroll .collapseitem .collbody .subcont .subicon { display:none; position:absolute; right:0; bottom:0; width:19px; height:15px; background-size:19px 15px; background-image:url("data:image/png;base64,"); }
.print-scroll .collapseitem .collbody .subcont.active { color:#1677ff; background-color:#e7f1ff; }
.print-scroll .collapseitem .collbody .subcont.active .subicon { display:block; }
.print-scroll .collapseitem .collbody .subcont.active.disabled { opacity:0.6; }



.popup-printbox .myScrollBox .t-radio-group .t-radio { --td-radio-icon-size:22px; padding:14px 16px !important; }
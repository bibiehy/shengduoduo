.myCommonPopupOuter {
	--td-popup-bg-color:#fff;
	--td-popup-border-radius:12px;
	overflow:hidden;
}
.myCommonPopup { display:flex; flex-direction:column; width:100%; min-height:30vh; max-height:60vh; background-color:#fff; overflow:hidden; }
.myCommonPopup .commonHeader { display:flex; align-items:center; justify-content:space-between; height:52px; box-shadow:0 5px 10px #e2e1e1; }
.myCommonPopup .commonHeader .myCancelBtn { line-height:52px; font-size:16px; color:#1677ff; padding:0 20px; }
.myCommonPopup .commonHeader .mySureBtn { line-height:52px; font-size:16px; color:#1677ff; padding:0 20px; }
.myCommonPopup .commonHeader .myTitle { flex:1; text-align:center; font-size:17px; color:rgba(0, 0, 0, 0.9); font-weight:600; }
.myCommonPopup .commonScroll { flex:1; width:calc(100% + 20px); padding:10px 25px 10px 5px; overflow-y:auto; }
.myCommonPopup .commonScroll .itemcont { position:relative; display:inline-block; width:calc(20% - 10px); height:40px; line-height:40px; color:#333; font-size:15px; background-color:#f5f5f5; border-radius:4px; text-align:center; box-sizing:border-box; margin:5px; }
.myCommonPopup .commonScroll .itemcont .itemicon { display:none; position:absolute; right:0; bottom:0; width:19px; height:15px; background-size:19px 15px; background-image:url("data:image/png;base64,"); }
.myCommonPopup .commonScroll .itemcont.active { color:#1677ff; background-color:#e7f1ff; }
.myCommonPopup .commonScroll .itemcont.active .itemicon { display:block; }
.myCommonPopup .commonScroll .itemcont.active.disabled { opacity:0.6; }


.myCommonPopup .commonScroll .kabanTipsCheckbox { border-bottom:solid 1px #f3f2f2; padding:0 10px 5px 10px; margin-bottom:5px; --td-checkbox-vertical-padding:0; --td-checkbox-font-size:16px; --td-checkbox-icon-size:24px; }
.myCommonPopup .commonScroll .kabanTipsCheckbox .kbtips { display:flex; align-items:center; font-size:16px; color:rgba(0, 0, 0, 0.85); }
.myCommonPopup .commonScroll .kabanTipsCheckbox .kbtips .note { font-size:14px; color:rgba(0, 0, 0, 0.45); margin-left:7px; }



.pageuser-box .listitem { position:relative; border:solid 1px #ebebeb; background-color:#fff; border-radius:8px; padding:12px 15px; margin-bottom:10px; }
.pageuser-box .listitem .status { position:absolute; right:18px; top:12px; }
.pageuser-box .listitem .psname { width:80%; font-size:16px; color:rgba(0, 0, 0, 0.85); padding-bottom:1px; white-space:nowrap; text-overflow:ellipsis; overflow:hidden; }
.pageuser-box .listitem .pstime { font-size:12px; color:rgba(0, 0, 0, 0.45); margin-top:4px; }
.pageuser-box .listitem .psinfo { display:flex; align-items:center; font-size:14px; color:rgba(0, 0, 0, 0.55); margin-top:4px; }
.pageuser-box .listitem .psreason { width:calc(100% - 70px); font-size:14px; color:rgba(0, 0, 0, 0.55); margin-top:4px; }
.pageuser-box .listitem .moneybox { position:absolute; right:15px; bottom:12px; text-align:center; }
.pageuser-box .listitem .moneybox .price { display:flex; align-items:flex-end; color:#ff3b30; font-size:20px; font-weight:600; }
.pageuser-box .listitem .moneybox .price .CNY { display:block; font-size:12px; padding-bottom:2px; }
.pageuser-box .listitem .moneybox .pricetxt { font-size:12px; margin-bottom:4px; }
.pageuser-box .listitem .moneybox .listag { display:flex; justify-content:center; min-width:60px !important; margin:0 auto; }
.pageuser-box .nodata { height:500px; }

.popup-profit { padding:8px 15px; }
.popup-profit .fenjianitem { display:flex; align-items:center; font-size:15px; color:rgba(0, 0, 0, 0.85); border-bottom:solid 1px #f0f0f0; padding:10px; }
.popup-profit .fenjianitem .huizong { display:block; color:#16ff; font-size:16px; padding:0 2px; }
.popup-profit02 { padding:10px; }
.popup-profit02 .reportitem { background-color:#fff; border-radius:5px; padding:12px 15px; margin-bottom:12px; }
.popup-profit02 .reportitem .viewname { display:flex; justify-content:space-between; align-items:center; font-size:17px; color:rgba(0, 0, 0, 0.85); font-weight:600; }
.popup-profit02 .reportitem .viewname .tongji { font-size:14px; color:rgba(0, 0, 0, 0.35); }
.popup-profit02 .reportitem .viewname .tongji .money { font-size:17px; color:#ff3b30; }
.popup-profit02 .reportitem .viewcontent { margin-top:4px; }
.popup-profit02 .reportitem .viewcontent .subitem { font-size:15px; color:rgba(0, 0, 0, 0.65); padding:2px 0; }
.popup-profit02 .reportitem .viewcontent .subitem .guige { color:#1677ff; }
.popup-profit02 .reportitem .viewcontent .subitem02 { display:flex; padding:2px 0; }
.popup-profit02 .reportitem .viewcontent .subitem02 .sublabel { font-size:15px; color:rgba(0, 0, 0, 0.65); }
.popup-profit02 .reportitem .viewcontent .subitem02 .subvalue .profile { color:#1677ff; padding-bottom:2px; }
.popup-profit02 .reportitem .viewcontent .subitem02 .subvalue .profile .huizong { color:#ff3b30; }
.popup-profit02 .reportitem .viewcontent .subitem02 .subimgs { display:flex; flex-wrap:wrap; }
.popup-profit02 .reportitem .viewcontent .subitem02 .subimgs .repimg { display:block; width:72px; height:72px; border:solid 1px #e8e8e8; border-radius:3px; padding:2px; margin-right:5px; }


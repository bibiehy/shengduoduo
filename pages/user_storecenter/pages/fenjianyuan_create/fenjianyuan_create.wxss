.fenjianyuan-createbox { display:flex; flex-direction:column; width:100vw; height:100vh; overflow:hidden; }
.fenjianyuan-createbox .mainbox { flex:1; width:calc(100% + 20px); padding:10px 30px 180px 10px; overflow-y:auto; }
.fenjianyuan-createbox .mainbox .innerbox { min-height:100%; background-color:#fff; border-radius:8px; padding:10px 10px 50px; overflow:hidden; }
.fenjianyuan-createbox .mainbox .idcard .t-grid-item { width:auto !important; }
.fenjianyuan-createbox .mainbox .idcard .t-grid-item .t-upload__wrapper { width:250px !important; height:154px !important; background-size:100% 100%; }
.fenjianyuan-createbox .mainbox .idcard .t-grid-item .t-upload__wrapper .t-upload__add-icon { font-size:34px !important; color:rgba(255, 255, 255, 0.6) !important; background-color:rgba(0, 0, 0, 0.3) !important; }
.fenjianyuan-createbox .mainbox .idcard .t-grid-item .t-upload__wrapper .t-upload__add-icon .t-icon-add { background-color:#1677ff; border-radius:50%; padding:2px; }
.fenjianyuan-createbox .mainbox .idcard.frontimg .t-grid-item .t-upload__wrapper { background:url('data:image/jpeg;base64,') no-repeat 0 0 !important; }
.fenjianyuan-createbox .mainbox .idcard.backimg .t-grid-item .t-upload__wrapper { background:url('data:image/jpeg;base64,') no-repeat 0 0 !important; }


